.topnav {
  overflow: hidden;
  background-color: #9cc2e5;
}

.topnav a {
  float: center;
  color:#FEFEFE;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
}

.topnav a:hover {
  background-color: whitesmoke;
  color: black;
}


